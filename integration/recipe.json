{
  "authorizationURL": "https://auth-api.idpartner.com/oidc-proxy/proxy/auth",
  "tokenURL": "https://auth-api.idpartner.com/oidc-proxy/proxy/token",
  "permissions": [
    {
      "scope": "openid",
      "label": "Protocol OpenID Connect",
      "required": true,
      "default": true
    },
    {
      "scope": "email",
      "label": "Email",
      "required": false,
      "default": true
    },
    {
      "scope": "profile",
      "label": "Full name",
      "required": false,
      "default": true
    }
  ],
  "configuration": [
    {
      "name": "client_id",
      "label": "Client ID",
      "description": "Public unique identifier for this application.",
      "required": true
    },
    {
      "name": "client_secret",
      "label": "Client Secret",
      "description": "Secret for this application.",
      "required": true
    }
  ]
}
